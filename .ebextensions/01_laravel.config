commands:
  01clearvendor:
    command: "rm -rf /var/app/staging/vendor"

container_commands:
  01-set-permissions:
    command: |
      sudo chown -R webapp:webapp /var/app/current
      sudo chmod -R 755 /var/app/current
  02-clear-cache:
    command: |
      php artisan config:clear
      php artisan cache:clear
      php artisan config:cache
